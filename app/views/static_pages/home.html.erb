<h1><%= t ".home" %></h1>
<% if logged_in? %>
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
        <%= render "shared/user_info" %>
      </section>
    </aside>
  </div>
  <div class="col-md-8">
    <%= render "shared/feed" %>
  </div>
<% else %>
  <%= link_to t(".signup"), signup_path, class: "btn btn-lg btn-primary" %>
<% end %>

